{
  "name": "aura-beam-annotator",
  "version": "0.7.4",
  "description": "A React component to highlight and annotate text",
  "author": {
    "name": "Goduu"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Goduu/aura-beam-annotator.git"
  },
  "main": "dist/index.js",
  "types": "dist/types/src/index.d.ts",
  "keywords": [
    "react",
    "highlight",
    "annotation",
    "ui component",
    "tailwind",
    "typescript"
